class Nodes:
    def __init__:
    left
    Nodes right
    Integer content
    self.setContent(content);
def Nodes getLeft():
    return left
def setLeft(Nodes left): 
    if left != null:
    self.links = left;
def Nodes getRight():
    return right
def setRight(Nodes right):
    if right != null:
    self.right = right
def int getContent():
    return content
def setContent(Integer content):
    self.content = content
def String toString():
    return "Content of Node : " + getContent()