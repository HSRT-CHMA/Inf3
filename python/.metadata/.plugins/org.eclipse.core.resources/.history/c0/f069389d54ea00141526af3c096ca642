class Nodes:
    
    left=0
    right=0
    content=0
    
    def __init__(self):
        self.setContent(content)
    def getLeft():
        return left
    def setLeft(Nodes left): 
        if left != None:
            self.left = left
    def Nodes getRight():
    return right
    def setRight(Nodes right):
    if right != None:
    self.right = right
    def int getContent():
    return content
    def setContent(Integer content):
    self.content = content
    def getContent(self):
        self.content= content
    def String toString():
    return "Content of Node : " + getContent()