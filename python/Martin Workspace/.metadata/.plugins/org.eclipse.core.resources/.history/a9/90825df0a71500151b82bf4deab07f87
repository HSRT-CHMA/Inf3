from RandomValue import RandomValue

class TestAVL():
    def __init__(self, avlTree = None):
        self.tree = avlTree
        
        '''
    the method for the integer test for the AVL tree fill the tree with random values 
    ''' 
    def int_test(self, tree):
        i = 0
        it = []
        randomV = RandomValue()
        while i < 10:    
            data = randomV.rFloat()
            tree.insert(data)
            it.insert(i, data)
            i = i+1
        for x in it:
            print(tree.has(x))