from AVLTree import AVLTree
from BinaryTree import BinaryTree
from RandomValue import RandomValue

class TestTree():
    
    def __init__(self):
        print("Starting with the Test!")
    
    '''
    the method for the integer test for the AVL tree fill the tree with random values 
    ''' 
    def int_test(self, tree):
        i = 0
        it = []
        randomV = RandomValue()
        while i < 10:    
            data = randomV.rFloat()
            tree.insert(data)
            it.insert(i, data)
            i = i+1
        for x in it:
            print(tree.has(x))
            
    def string_test(self, tree):
        i = 0
        st = []
        while i < 10:    
            data = self.random_string_generator()
            tree.insert(data)
            st.insert(i, data)
            i = i+1
             
        for x in st:
          
            print(tree.has(x))